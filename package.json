{
  "name": "covid-19",
  "version": "1.0.0",
  "description": "A GraphQL server for COVID-19 time series data for counties in the United States.",
  "private": true,
  "engines": {
    "npm": "6.x",
    "node": "12.x"
  },
  "main": "lib/server.js",
  "keywords": [
    "covid-19",
    "api",
    "graphql",
    "graphql-server",
    "mongodb-database",
    "mongo-database",
    "parsing"
  ],
  "author": "Kevin Lazich <dev@kevinlazich.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/klazich/covid-19"
  },
  "scripts": {
    "build": "shx rm -rf lib && babel src -d lib",
    "commit": "commit",
    "format": "prettier --write \"src/**/*.{js,json,md}\"",
    "lint": "eslint src --ext .js --cache",
    "lint:fix": "npm run lint -- --fix",
    "preseed": "npm run build",
    "seed": "node -r dotenv-safe/config lib/database/seed",
    "prestart": "npm run build",
    "start": "node -r dotenv-safe/config lib/server/index.js"
  },
  "dependencies": {
    "apollo-server": "^2.16.1",
    "axios": "^0.19.2",
    "dotenv-safe": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.2.6",
    "minimist": "^1.2.5",
    "mongodb": "^3.6.0",
    "mongoose": "^5.10.0",
    "papaparse": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@commitlint/prompt-cli": "^9.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5",
    "shx": "^0.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
